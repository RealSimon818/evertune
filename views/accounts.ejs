<!-- views/accounts.ejs -->
<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <title>Frozen Accounts</title>
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet">
</head>
<body>
  <div class="container mt-5">
    <h2 class="mb-4 text-center">Frozen Accounts </h2>

    <table class="table table-bordered table-striped">
      <thead class="table-dark">
        <tr>
          <th>Username</th>
         
          <th>Total Balance</th>
          <th>Freezing Point</th>
          <th>Latest Optimization Count</th>
          <th>VIP Level</th>
          <th>Daily Limit</th>
          <th>Today's Profit</th>
        </tr>
      </thead>
      <tbody>
        <% if (accounts.length > 0) { %>
          <% accounts.forEach(account => { %>
            <tr>
              <td><%= account.username %></td>

              <td><%= account.totalBalance %></td>
              <td><%= account.freezingPoint %></td>
              <td><%= account.latestOptimizationCount %></td>
              <td><%= account.vipLevel %></td>
              <td><%= account.dailyLimit %></td>
              <td><%= account.todaysProfit %></td>
            </tr>
          <% }) %>
        <% } else { %>
          <tr>
            <td colspan="8" class="text-center">No accounts meet the criteria.</td>
          </tr>
        <% } %>
      </tbody>
    </table>
  </div>

  <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js"></script>
</body>
</html>
