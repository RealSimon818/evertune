<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>EverTune ¬∑ About us ‚Äì AI brand optimization</title>
    <!-- Google Fonts: Montserrat, Manrope, Red Hat, DM Sans, fallbacks -->
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Montserrat:ital,wght@0,100..900;1,100..900&family=Manrope:wght@300..700&family=Red+Hat+Display:wght@300..700&family=DM+Sans:wght@500&family=Roboto:wght@300..700&family=Source+Sans+Pro:wght@300..700&display=swap" rel="stylesheet">
    <!-- Bootstrap Icons (for clean, rounded icons) -->
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.11.3/font/bootstrap-icons.min.css">
    <style>
        * {
            margin: 0;
            padding: 0;
            box-sizing: border-box;
        }

        body {
            background-color: #FFFFFF;
            color: #1B1B1B;
            font-family: 'Manrope', 'Roboto', 'Source Sans Pro', sans-serif;
            line-height: 1.6;
            -webkit-font-smoothing: antialiased;
        }

        .container-base {
            max-width: 1200px;
            margin: 0 auto;
            padding: 2rem 1.5rem;
        }

        /* same gradient style as terms/faq */
        .about-gradient-bg {
            background: radial-gradient(circle at 0% 10%, rgba(255,249,246,0.9) 0%, rgba(249,220,183,0.3) 40%, rgba(105,215,222,0.1) 80%);
            background-blend-mode: plus-darker;
        }

        /* header with back button & brand */
        .about-header {
            display: flex;
            align-items: center;
            justify-content: space-between;
            flex-wrap: wrap;
            margin-bottom: 2rem;
        }

        .back-btn {
            display: inline-flex;
            align-items: center;
            gap: 0.5rem;
            background-color: transparent;
            border: 1.5px solid #1B1B1B;
            color: #1B1B1B;
            font-family: 'Montserrat', sans-serif;
            font-weight: 500;
            font-size: 1rem;
            padding: 0.6rem 1.6rem;
            border-radius: 50px;
            cursor: pointer;
            transition: all 0.2s ease;
            text-decoration: none;
            box-shadow: 0 2px 6px rgba(0,0,0,0.02);
        }

        .back-btn:hover {
            background-color: #F7594E;
            border-color: #F7594E;
            color: white;
            transform: scale(1.02);
            box-shadow: 0 8px 18px rgba(247, 89, 78, 0.15);
        }

        .back-arrow {
            font-size: 1.3rem;
            line-height: 1;
        }

         /* EverTune logo / brand word */
        .logo-wrapper {
            display: flex;
            align-items: center;
        }
        .evertune-logo {
            width: 136px;
            height: 32px;
        }

        h1 {
            font-family: 'Montserrat', 'Manrope', sans-serif;
            font-weight: 700;
            font-size: 2.6rem;
            margin: 0.2rem 0 0.25rem;
            line-height: 1.2;
        }

        .tagline {
            font-size: 1.2rem;
            color: #3a3a3a;
            margin-bottom: 2rem;
            border-bottom: 1px solid rgba(0,0,0,0.05);
            padding-bottom: 1.2rem;
            font-style: italic;
        }

        /* STATS GRID (4 items) */
        .stats-grid {
            display: grid;
            grid-template-columns: repeat(4, 1fr);
            gap: 1.5rem;
            margin: 2.5rem 0 2rem;
        }

        .stat-card {
            background: white;
            border-radius: 32px;
            padding: 1.5rem 1rem;
            text-align: center;
            box-shadow: 0 8px 22px rgba(0,0,0,0.02);
            border: 1px solid rgba(255,255,255,0.7);
            transition: all 0.2s;
        }

        .stat-card:hover {
            box-shadow: 0 12px 30px rgba(247, 89, 78, 0.08);
            border-color: #F7594E20;
        }

        .stat-number {
            font-family: 'Montserrat', sans-serif;
            font-weight: 700;
            font-size: 2.4rem;
            color: #F7594E;
            line-height: 1.2;
        }

        .stat-label {
            font-family: 'Manrope', sans-serif;
            font-weight: 500;
            color: #1B1B1B;
            font-size: 1rem;
            text-transform: uppercase;
            letter-spacing: 0.03em;
        }

        /* section titles */
        .section-title {
            font-family: 'Montserrat', sans-serif;
            font-weight: 700;
            font-size: 2rem;
            margin: 3rem 0 1.5rem 0;
            position: relative;
            display: inline-block;
        }
        .section-title:after {
            content: '';
            display: block;
            width: 70px;
            height: 4px;
            background: linear-gradient(90deg, #F7594E, #69D7DE);
            border-radius: 4px;
            margin-top: 6px;
        }

        .section-content {
            background: #FFFFFFdd;
            backdrop-filter: blur(2px);
            border-radius: 36px;
            padding: 2rem 2.2rem;
            box-shadow: 0 4px 20px rgba(0,0,0,0.02);
            border: 1px solid #ffffff;
        }

        .section-content p {
            font-size: 1.1rem;
            margin-bottom: 1.2rem;
            color: #2a2a2a;
        }

        /* core values grid (6 cards) */
        .values-grid {
            display: grid;
            grid-template-columns: repeat(3, 1fr);
            gap: 1.8rem;
            margin: 2rem 0 1rem;
        }

        .value-card {
            background: white;
            border-radius: 32px;
            padding: 2rem 1.5rem;
            box-shadow: 0 8px 22px rgba(0, 0, 0, 0.02);
            transition: transform 0.15s ease, box-shadow 0.2s;
            border: 1px solid #f0f0f0;
            text-align: center;
        }

        .value-card:hover {
            transform: translateY(-5px);
            box-shadow: 0 20px 30px rgba(247,89,78,0.06);
            border-color: #F7594E20;
        }

        .value-icon {
            font-size: 2.8rem;
            color: #F7594E;
            background: #FFF9F6;
            padding: 0.5rem;
            border-radius: 60px;
            display: inline-block;
            margin-bottom: 1.2rem;
        }

        .value-title {
            font-family: 'Montserrat', sans-serif;
            font-weight: 600;
            font-size: 1.4rem;
            margin-bottom: 0.8rem;
        }

        .value-card p {
            color: #3a3a3a;
            font-size: 0.98rem;
        }

        /* steps (how it works) */
        .steps-container {
            display: grid;
            grid-template-columns: repeat(2, 1fr);
            gap: 1.5rem;
            margin: 2rem 0 2.5rem;
        }

        .step-card {
            background: white;
            border-radius: 40px;
            padding: 1.5rem 1.8rem;
            display: flex;
            align-items: flex-start;
            gap: 1.2rem;
            box-shadow: 0 8px 18px rgba(0,0,0,0.02);
            border: 1px solid rgba(105,215,222,0.2);
        }

        .step-number {
            background: #F7594E;
            color: white;
            width: 48px;
            height: 48px;
            border-radius: 60px;
            display: flex;
            align-items: center;
            justify-content: center;
            font-family: 'Montserrat', sans-serif;
            font-weight: 700;
            font-size: 1.8rem;
            flex-shrink: 0;
            box-shadow: 0 6px 14px rgba(247,89,78,0.3);
        }

        .step-content h5 {
            font-family: 'Montserrat', sans-serif;
            font-weight: 600;
            font-size: 1.3rem;
            margin-bottom: 0.4rem;
        }

        .step-content p {
            color: #3a3a3a;
            margin-bottom: 0;
        }

        /* get in touch section */
        .get-in-touch-section {
            background: linear-gradient(120deg, #FFF9F6, #FFFFFF);
            border-radius: 48px;
            padding: 3rem 2.5rem;
            text-align: center;
            margin: 3rem 0 2rem;
            border: 1px solid rgba(247,89,78,0.2);
            box-shadow: 0 20px 40px rgba(0,0,0,0.02);
        }

        .get-in-touch-section h3 {
            font-family: 'Montserrat', sans-serif;
            font-weight: 700;
            font-size: 2.2rem;
            margin-bottom: 1rem;
        }

        .get-in-touch-section p {
            font-size: 1.15rem;
            max-width: 600px;
            margin: 0 auto 1.8rem;
            color: #2a2a2a;
        }

        .contact-support-btn {
            background-color: #F7594E;
            border: none;
            color: white;
            font-family: 'Montserrat', sans-serif;
            font-weight: 600;
            font-size: 1.2rem;
            padding: 1rem 2.8rem;
            border-radius: 60px;
            display: inline-flex;
            align-items: center;
            gap: 0.7rem;
            cursor: pointer;
            transition: all 0.2s ease;
            box-shadow: 0 8px 20px rgba(247, 89, 78, 0.3);
            border: 1px solid transparent;
        }

        .contact-support-btn i {
            font-size: 1.5rem;
        }

        .contact-support-btn:hover {
            background-color: #e03e35;
            transform: scale(1.02);
            box-shadow: 0 12px 28px rgba(247,89,78,0.4);
        }

        .text-muted {
            color: #6b6b6b;
            font-size: 0.95rem;
        }

        .mt-4 {
            margin-top: 2rem;
        }

        .text-center {
            text-align: center;
        }

        /* footer */
        .about-footer {
            margin-top: 3rem;
            text-align: center;
            font-size: 0.9rem;
            color: #4a4a4a;
            border-top: 1px solid rgba(0,0,0,0.05);
            padding-top: 2rem;
            font-family: 'Manrope', sans-serif;
        }

        .about-footer a {
            color: #1B1B1B;
            text-decoration: none;
            border-bottom: 1px dotted #F7594E;
        }
        .about-footer a:hover {
            color: #F7594E;
        }

        /* responsive */
        @media (max-width: 900px) {
            .values-grid {
                grid-template-columns: repeat(2, 1fr);
            }
        }

        @media (max-width: 700px) {
            h1 { font-size: 2.2rem; }
            .stats-grid { grid-template-columns: repeat(2, 1fr); gap: 1rem; }
            .values-grid { grid-template-columns: 1fr; }
            .steps-container { grid-template-columns: 1fr; }
            .step-card { align-items: center; }
            .get-in-touch-section { padding: 2rem 1.5rem; }
            .section-title { font-size: 1.8rem; }
        }

        @media (max-width: 480px) {
            .stats-grid { grid-template-columns: 1fr; }
        }

        /* additional highlight */
        .coral-badge {
            background: #F7594E10;
            color: #F7594E;
            border-radius: 50px;
            padding: 0.2rem 1rem;
            font-weight: 500;
            font-size: 0.85rem;
            display: inline-block;
            margin-left: 0.5rem;
        }
    </style>
</head>
