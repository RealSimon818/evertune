        })
      });
      
      const result = await response.json();
      
      if (result.success) {
        // Update optimization count in UI - FIX: Increment properly
        const countElement = document.querySelector('.optimization-count-display span:first-child');
        if (countElement) {
          let current = parseInt(countElement.textContent) || 0;
          countElement.textContent = current + 1;
        }
        
        // Update the count badge
        const badgeElement = document.querySelector('.optimization-count-badge');
        if (badgeElement) {
          let badgeText = badgeElement.textContent;
          let matches = badgeText.match(/(\d+)\/(\d+)/);
          if (matches) {
            let current = parseInt(matches[1]) || 0;
            let max = parseInt(matches[2]) || 40;
            badgeElement.textContent = `${current + 1}/${max}`;
          }
        }
        
        // Close modal
        const modalElement = document.getElementById('imageModal');
        const modal = bootstrap.Modal.getInstance(modalElement);
        if (modal) modal.hide();
        
        // Show success and reload
        setTimeout(() => window.location.reload(), 500);
      } else {
        displayError(result.message || 'Failed to update progress');
      }
    } catch (error) {
      console.error('Error:', error);
      displayError('Network error. Please try again.');
    }
  }
  
